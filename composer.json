{
    "name": "touge/admin-sundry",
    "description": "基于laravel-admin的在线学习插件。提供在线时长监控和签到功能",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/touge/admin-sundry",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Yuan",
            "email": "lon.y@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Touge\\AdminSundry\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Touge\\AdminSundry\\AdminSundryServiceProvider"
            ]
        }
    }
}
